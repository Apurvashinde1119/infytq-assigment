def make_amount(rupees_to_make,no_of_five,no_of_one):
    five_needed=0
    one_needed=0
     flag=0
    #Start writing your code here
     if ruppes_to_make<=(no_of_five*5+no_of_one):
         five=ruppes_to_make//5
         ones=ruppes_to_make%5
         if fives<=no_of_five and ones<=no_of_one:
             five_needed=fives
             one_needed=ones
             flag=1   
             elif fives>no_of_five:
                 five_needed=no_of_five
                 one_needed=ruppes_to_make*(five_needed*5)
                 flag=1 
                 
        if flag==1
        
        
            print("no. of five needed  :",five_needed)
            print("no. of  one needed :",one_needed)
        else:
            print(-1)
     make_amount(28,8,5)
